#include <stdio.h>

typedef int(AINT5)[5];
typedef float(AFLOAT10)[10];
typedef char(ACHAR9)[9];

int main()
{
		AINT5 a1;
		float fArray[10];
		AFLOAT10* pf = &fArray;
		ACHAR9 cArray;
		
		char(*pc)[9] = &cArray;
		char(*pcw)[4] = cArray;		// error
		
		int i = 0;
		
		printf("%d, %d\n", sizeof(AINT5), sizeof(a1));	// 20, 20
		
		for(i=0; i<10; i++)
		{
				(*pf)[i] = i;		// ==> fArray[i] = i;
		}
		
		for(i=0; i<10; i++)
		{
				printf("%f\n", fArray[i]);	//´òÓ¡ 0 µ½ 9
		}
		
		printf("%p, %p, %p\n", &cArray, pc+1, pcw+1);		// pc +1 ==>(unsigned int)pc + sizeof(pc) + 9;
		
		return 0;
}